{
    "name": "ts-discordjs",
    "version": "1.0.0",
    "description": "A template for typescript discord bot",
    "main": "dist/index.js",
    "scripts": {
        "test": "tsc-watch --onCompilationComplete \"node dist/index.js\"",
        "start": "node dist/index.js",
        "build": "rimraf dist/* && tsc",
        "lint": "eslint \"src/**\"",
        "postinstall": "npm run build && node dist/setup.js"
    },
    "engines": {
        "node": ">=12"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Linux123123/ts-discordjs.git"
    },
    "keywords": [
        "ts-discordjs",
        "typescript",
        "discordjs"
    ],
    "author": "Linux123123",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Linux123123/ts-discordjs/issues"
    },
    "homepage": "https://github.com/Linux123123/ts-discordjs#readme",
    "dependencies": {
        "@types/node": "^14.14.31",
        "@types/readline-sync": "^1.4.3",
        "@types/ws": "^7.4.0",
        "chalk": "^4.1.0",
        "discord.js": "^12.5.1",
        "enmap": "^5.8.4",
        "moment": "^2.29.1",
        "readline-sync": "^1.4.10",
        "rimraf": "^3.0.2",
        "typescript": "^4.2.3"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^4.15.2",
        "@typescript-eslint/parser": "^4.15.2",
        "eslint": "^7.21.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "prettier": "^2.2.1",
        "tsc-watch": "^4.2.9"
    }
}
